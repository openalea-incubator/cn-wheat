{
    "version": 1,
    "formatters": {
        "verbose": {
            "format": "%(levelname)s - %(name)s - %(message)s"
        },
        "simple": {
            "format": "%(message)s"
        }
    },
    "handlers": {
        "console": {
            "level": "WARNING",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout",
            "formatter": "verbose"
        },
        "file_info": {
            "level": "INFO",
            "class": "logging.FileHandler",
            "filename": "info.log",
            "mode": "w",
            "formatter": "verbose"
        },
        "file_debug": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "debug.log",
            "mode": "w",
            "formatter": "verbose"
        },
        "plants_compartments_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "plants_compartments.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "axes_compartments_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "axes_compartments.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "phytomers_compartments_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "phytomers_compartments.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "organs_compartments_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "organs_compartments.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "elements_compartments_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "elements_compartments.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "plants_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "plants_derivatives.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "axes_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "axes_derivatives.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "phytomers_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "phytomers_derivatives.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "organs_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "organs_derivatives.csv",
            "mode": "w",
            "formatter": "simple"
        },
        "elements_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "elements_derivatives.csv",
            "mode": "w",
            "formatter": "simple"
        }
    },
    "loggers": {
        "cnwheat": {
            "handlers": [
                "console",
                "file_info",
                "file_debug"
            ]
        },
        "cnwheat.model": {
            "handlers": [
                "file_debug"
            ],
            "propagate": false
        },
        "cnwheat.compartments": {
            "propagate": false
        },
        "cnwheat.compartments.plants": {
            "handlers": [
                "plants_compartments_file"
            ]
        },
        "cnwheat.compartments.axes": {
            "handlers": [
                "axes_compartments_file"
            ]
        },
        "cnwheat.compartments.phytomers": {
            "handlers": [
                "phytomers_compartments_file"
            ]
        },
        "cnwheat.compartments.organs": {
            "handlers": [
                "organs_compartments_file"
            ]
        },
        "cnwheat.compartments.elements": {
            "handlers": [
                "elements_compartments_file"
            ]
        },
        "cnwheat.derivatives": {
            "propagate": false
        },
        "cnwheat.derivatives.plants": {
            "handlers": [
                "plants_derivatives_file"
            ]
        },
        "cnwheat.derivatives.axes": {
            "handlers": [
                "axes_derivatives_file"
            ]
        },
        "cnwheat.derivatives.phytomers": {
            "handlers": [
                "phytomers_derivatives_file"
            ]
        },
        "cnwheat.derivatives.organs": {
            "handlers": [
                "organs_derivatives_file"
            ]
        },
        "cnwheat.derivatives.elements": {
            "handlers": [
                "elements_derivatives_file"
            ]
        }
    }
}